tshark -r dos_4_final.pcap -T fields -e frame.number -e frame.time -e frame.time_delta -e frame.time_relative -e frame.protocols -e frame.len -e ip.src -e ip.dst -e ipv6.src -e ipv6.dst -e eth.src -e eth.dst -e ip.proto -e udp.srcport -e udp.dstport -e coap.code -e coap.type -e coap.mid -e coap.opt.delta -e coap.opt.observe -e coap.opt.length -e coap.response_in -e coap.response_to -e coap.payload -e coap.payload_desc -e coap.payload_length -e coap.opt.uri_path -e text -E header=y -E separator=, -E quote=d -E occurrence=a > coap_dos.csv